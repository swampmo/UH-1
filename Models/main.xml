<!-- Removes reflective glass on internal views -->
   <animation>
        <type>select</type>
       <object-name>vitres</object-name>
	   <object-name>frontdoorglasleft</object-name>
       <object-name>frontdoorglasright</object-name>
       <object-name>sidedoorglasleft</object-name>
       <object-name>sidedoorglasright</object-name>
       <object-name>slidedoorglasleft</object-name>
       <object-name>slidedoorglasright</object-name>
       <object-name>vitresint</object-name>
       <object-name>frontdoorglasleftint</object-name>
       <object-name>sidedoorglasleftint</object-name>
       <object-name>slidedoorglasleftint</object-name>
       <object-name>frontdoorglasrighint</object-name>
       <object-name>sidedoorglasrightint</object-name>
       <object-name>slidedoorglasrighint</object-name>

		<condition>
            <not>
            <property>sim/current-view/internal</property>
            </not>
        </condition>
    </animation>

 <!-- payloads -->
<model>
<path>Aircraft/UH-1/Models/payload/loads.xml</path>
<offsets>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </offsets>
</model> 

<!-- Gouverne de profondeur -->
<animation>
<type>rotate</type>
<object-name>elevatortrim</object-name>
<property>surface-positions/elevator-pos-norm</property>
<interpolation>
<entry><ind> -0.75 </ind><dep>  -1.5 </dep></entry>
<entry><ind>  0.00 </ind><dep>   0.0 </dep></entry>
<entry><ind>  0.75 </ind><dep>   1.5 </dep></entry>
</interpolation>
<center>
<x-m>  1.201 </x-m>
<y-m>  0.000 </y-m>
<z-m> -0.106 </z-m>
</center>
<axis>
<x> 0 </x>
<y> 1 </y>
<z> 0 </z>
</axis>
</animation>

 <animation>
        <type>select</type>
        <object-name>pilot1</object-name>
		<object-name>pilot2</object-name>
		<condition>
         <not>
            <property>sim/current-view/internal</property>
         </not>
        </condition>
    </animation>	
                               
