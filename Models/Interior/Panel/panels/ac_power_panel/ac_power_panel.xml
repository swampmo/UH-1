  <path>ac_power_panel.ac</path>

  <animation>
    <type>material</type>
    <object-name>invtrswitch</object-name>
    <object-name>panel</object-name>
    <object-name>phasepoti</object-name>
    <emission>
      <factor-prop>controls/lighting/instltg/console</factor-prop>
      <red>  0.04</red>
      <green>0.0</green>
      <blue> 0.0</blue>
    </emission>
  </animation>

