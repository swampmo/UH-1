  <animation>
    <type>knob</type>
    <object-name>invtrswitch</object-name>
    <property>controls/switches/invtr</property>
    <center>
      <x-m>0.000</x-m>
      <y-m>0.047</y-m>
      <z-m>0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>30</factor>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/switches/invtr</property>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

