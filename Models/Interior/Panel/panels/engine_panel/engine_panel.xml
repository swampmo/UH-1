<?xml version="1.0" ?>

<PropertyList>

  <path>engine_panel.ac</path>

  <animation>
    <type>material</type>
    <object-name>clutchguard</object-name>
    <object-name>deiceswitch</object-name>
    <object-name>govswitch</object-name>
    <object-name>intauxfuelleftswitch</object-name>
    <object-name>intauxfuelrightswitch</object-name>
    <object-name>lowrpmswitch</object-name>
    <object-name>mainswitch</object-name>
    <object-name>panel</object-name>
    <object-name>startswitch</object-name>
    <emission>
      <factor-prop>controls/lighting/instltg/ped</factor-prop>
      <red>  0.4</red>
      <green>0.0</green>
      <blue> 0.0</blue>
    </emission>
  </animation>

<!-- Clutch Guard -->
  <animation>
    <type>knob</type>
    <object-name>clutchguard</object-name>
    <property>controls/switches/engine/clutchguard</property>
    <center>
      <x-m>-0.006</x-m>
      <y-m> 0.000</y-m>
      <z-m> 0.001</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>85</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/engine/clutchguard</property>
      </binding>
    </action>
  </animation>

<!-- Main Fuel Switch -->
  <animation>
    <type>knob</type>
    <object-name>mainswitch</object-name>
    <property>controls/switches/fuel/transfer-pump[1]</property>
    <center>
      <x-m>-0.022</x-m>
      <y-m> 0.000</y-m>
      <z-m> 0.001</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/fuel/transfer-pump[1]</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

<!-- Start Fuel Switch -->
  <animation>
    <type>knob</type>
    <object-name>startswitch</object-name>
    <property>controls/switches/engine/startfuel</property>
    <center>
      <x-m>-0.022</x-m>
      <y-m> 0.000</y-m>
      <z-m> 0.001</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/engine/startfuel</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

<!-- Low RPM Switch -->
  <animation>
    <type>knob</type>
    <object-name>lowrpmswitch</object-name>
    <property>controls/switches/lowrpmaudio</property>
    <center>
      <x-m>-0.022</x-m>
      <y-m>-0.054</y-m>
      <z-m> 0.001</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/lowrpmaudio</property>
      </binding>
      <binding>
        <script>uh1.rpm_warn()</script>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>


<animation>
    <type>knob</type>
    <object-name>intauxfuelrightswitch</object-name>
    <property>controls/switches/fuel/right-boost-pump[1]</property>
    <center>
      <x-m> 0.026</x-m>
      <y-m>-0.057</y-m>
      <z-m>-0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/fuel/right-boost-pump[1]</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>intauxfuelleftswitch</object-name>
    <property>controls/switches/fuel/left-boost-pump[1]</property>
    <center>
      <x-m> 0.026</x-m>
      <y-m>-0.054</y-m>
      <z-m>-0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/fuel/left-boost-pump[1]</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>deiceswitch</object-name>
    <property>controls/anti-ice/engine[1]/inlet-heat</property>
    <center>
      <x-m> 0.025</x-m>
      <y-m>-0.000</y-m>
      <z-m>-0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/anti-ice/engine[1]/inlet-heat</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

