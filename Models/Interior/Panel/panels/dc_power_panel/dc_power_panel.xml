<?xml version="1.0" ?>

<PropertyList>

  <path>dc_power_panel.ac</path>

  <animation>
    <type>material</type>
    <object-name>batswitch</object-name>
    <object-name>clutchguard</object-name>
    <object-name>clutchswitch</object-name> <!-- main generator -->
    <object-name>nonessswitch</object-name>
    <object-name>panel</object-name>
    <object-name>starterswitch</object-name>
    <object-name>vmpoti</object-name>
    <emission>
      <factor-prop>controls/lighting/instltg/console</factor-prop>
      <red>  0.4</red>
      <green>0.0</green>
      <blue> 0.0</blue>
    </emission>
  </animation>

<!-- battery switch -->
  <animation>
    <type>knob</type>
    <object-name>batswitch</object-name>
    <property>controls/switches/battery</property>
    <center>
      <x-m>0.029</x-m>
      <y-m>0.000</y-m>
      <z-m>0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/battery</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

<!-- clutch guard -->
  <animation>
    <type>knob</type>
    <object-name>clutchguard</object-name>
    <property>controls/switches/maingenclutchguard</property>
    <center>
      <x-m>-0.035</x-m>
      <y-m> 0.000</y-m>
      <z-m> 0.001</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-85</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/maingenclutchguard</property>
      </binding>
    </action>
  </animation>

<!-- main gen switch -->
  <animation>
    <type>knob</type>
    <object-name>clutchswitch</object-name>
    <property>controls/switches/main_gen</property>
    <center>
      <x-m>-0.012</x-m>
      <y-m> 0.000</y-m>
      <z-m> 0.001</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>30</factor>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/switches/main_gen</property>
        <min>-1</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

<!-- non ess. bus switch -->
  <animation>
    <type>knob</type>
    <object-name>nonessswitch</object-name>
    <property>controls/switches/nonessbus</property>
    <center>
      <x-m>0.029</x-m>
      <y-m>0.000</y-m>
      <z-m>0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/nonessbus</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

<!-- starter generator switch -->
  <animation>
    <type>knob</type>
    <object-name>starterswitch</object-name>
    <property>controls/switches/startergen</property>
    <center>
      <x-m>0.029</x-m>
      <y-m>0.000</y-m>
      <z-m>0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>30</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/startergen</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

<!-- vm selector -->
  <animation>
    <type>knob</type>
    <object-name>vmpoti</object-name>
    <property>controls/switches/vm</property>
    <center>
      <x-m>-0.015</x-m>
      <y-m> 0.034</y-m>
      <z-m> 0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
    <factor>60</factor>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/switches/vm</property>
        <min>0</min>
        <max>4</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

</PropertyList>
