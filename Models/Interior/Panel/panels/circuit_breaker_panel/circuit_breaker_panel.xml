<!-- avionic master switch -->
  <animation>
    <type>knob</type>
    <object-name>master_avionics_switch</object-name>
    <property>controls/switches/main_avionics</property>
    <factor>30</factor>
    <center>
      <x-m>-0.099</x-m>
      <y-m> 0.023</y-m>
      <z-m> 0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/main_avionics</property>
      </binding>
      <binding>
        <command>property-toggle</command>
        <property>sim/sound/click</property>
      </binding>
    </action>
  </animation>

