<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>screen.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>fond</object-name>

    <!-- Objets transparents -->
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>vitre</object-name>
    <emission>
      
      <red>   0.17 </red>
      <green> 0.63 </green>
      <blue>  0.17 </blue>
    </emission>
  </animation> 


 
  <animation>
    <type>material</type>
    <object-name>fond</object-name>
  
    <emission>
        <red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
<factor-prop>/controls/lighting/landing-lights</factor-prop>
    </emission>
  </animation>




</PropertyList>
