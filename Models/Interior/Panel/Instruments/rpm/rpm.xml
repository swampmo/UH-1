<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>rpm.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>Eneedle</object-name>
    <object-name>Rneedle</object-name>


  </animation>

<!--Working For Rembrandt-->

<animation>
<object-name>vitre</object-name>
</animation>

<animation>
<type>noshadow</type>
<object-name>vitre</object-name>
</animation>

<animation>
<type>shader</type>
<shader>chrome</shader>
<texture>Aircraft/Generic/Effects/glass_shader.png</texture>
<object-name>vitre</object-name>
</animation>
<!--Working For Rembrandt-->

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>Eneedle</object-name>
    <object-name>Rneedle</object-name>
    <emission>
      <red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
<factor-prop>/controls/lighting/dash-dim</factor-prop>
    </emission>
  </animation>






 
  <animation>
    <type>material</type>
    <object-name>fond</object-name>
  
    <emission>
        <red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
<factor-prop>/controls/lighting/landing-lights</factor-prop>
    </emission>
  </animation>






  <animation>
    <type>rotate</type>
    <object-name>Eneedle</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
         <entry><ind>   0 </ind><dep> 0 </dep></entry>
      <entry><ind> 162 </ind><dep> 11 </dep></entry>
      <entry><ind> 324 </ind><dep>   88 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m> -0.027 </y-m>
      <z-m>  0.000 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Rneedle</object-name>
    <property>rotors/main/rpm</property>
    <interpolation>
       <entry><ind>   0 </ind><dep> 0 </dep></entry>
      <entry><ind> 162 </ind><dep> 11 </dep></entry>
      <entry><ind> 324 </ind><dep>   88 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.000 </x-m>
      <y-m> 0.027 </y-m>
      <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

