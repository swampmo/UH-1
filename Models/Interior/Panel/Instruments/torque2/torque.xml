<?xml version="1.0"?>

<PropertyList>
	<path>torque.ac</path>
	
	  <animation>
        <type>material</type>
        <object-name>Face</object-name>
        <object-name>Lneedle</object-name>
        <object-name>Rneedle</object-name>
        <object-name>visses</object-name>
        <object-name>vitre</object-name>
        <object-name>fond</object-name>
       
        <emission>
            <red>1</red>
            <green>1</green>
            <blue>1</blue>
            <factor-prop>controls/lighting/dash-dim</factor-prop>
        </emission>
    </animation>
	
	
	
	<animation>
		<type>rotate</type>
		<object-name>Lneedle</object-name>
		<property>rotors/gear/total-torque</property>
		<interpolation>
			<entry><ind>0</ind><dep>-205</dep></entry>
			<entry><ind>160000</ind><dep>-90</dep></entry>
			<entry><ind>320000</ind><dep>25</dep></entry>
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>Rneedle</object-name>
		<property>rotors/gear/total-torque</property>
		<interpolation>
			<entry><ind>0</ind><dep>-205</dep></entry>
			<entry><ind>160000</ind><dep>-90</dep></entry>
			<entry><ind>320000</ind><dep>25</dep></entry>					
		</interpolation>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
    <animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>
	
</PropertyList>
