<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>voltmeter.ac</path>

  <animation>
    <object-name>frame</object-name>
    <object-name>face</object-name>
    <object-name>fond</object-name>
    <object-name>needle</object-name> 
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.25 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
     <factor-prop>controls/lighting/dash-dim</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>systems/electrical/volts</property>
    <interpolation>
      <entry><ind> 30 </ind><dep>  44.45 </dep></entry>
      <entry><ind>  0 </ind><dep> -45.44 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.000 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.011 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>

