<?xml version="1.0" ?>

<PropertyList>

  <path>arc51bx.ac</path>

  <animation>
    <type>material</type>
    <object-name>blend</object-name>
    <object-name>cap</object-name>
    <object-name>circlek010</object-name>
    <object-name>circlek100</object-name>
    <object-name>circlem001</object-name>
    <object-name>circlem010</object-name>
    <object-name>circlem100</object-name>
    <object-name>funcswitch</object-name>
    <object-name>panel</object-name>
    <object-name>presetchan</object-name>
    <object-name>presetswitch</object-name>
    <object-name>receiveswitch</object-name>
    <object-name>sqdisableswitch</object-name>
    <object-name>tune001</object-name>
    <object-name>tune010</object-name>
    <object-name>tune100</object-name>
    <object-name>volume</object-name>
    <emission>
      <factor-prop>controls/lighting/instltg/ped</factor-prop>
      <red>  0.01</red>
      <green>0.01</green>
      <blue> 0.01</blue>
    </emission>
  </animation>

  <animation>
    <type>knob</type>
    <object-name>cap</object-name>
    <property>controls/switches/arc51bx/cap</property>
    <center>
      <x-m>-0.063</x-m>
      <y-m>-0.003</y-m>
      <z-m> 0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-127</factor>
    <action>
      <binding>
        <command>property-toggle</command>
        <property>controls/switches/arc51bx/cap</property>
      </binding>
    </action>
  </animation>

