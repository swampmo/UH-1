  <animation>
    <type>knob</type>
    <object-name>wheel1</object-name>
    <property>instrumentation/transponder/inputs/digit</property>
    <center>
      <x-m> 0.0472</x-m>
      <y-m> 0.0000</y-m>
      <z-m>-0.0193</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
    <factor>-36</factor>
    <increase>
      <binding>
        <command>nasal</command>
        <script>apx72.setdigit( "instrumentation/transponder/inputs/digit", 1 );</script>
      </binding>
    </increase>
    <decrease>
      <binding>
        <command>nasal</command>
        <script>apx72.setdigit( "instrumentation/transponder/inputs/digit", 0 );</script>
      </binding>
    </decrease>
  </animation>


