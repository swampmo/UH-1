<PropertyList>

  <path>apx72.ac</path>

  <animation>
    <type>material</type>
    <object-name>panel</object-name>
    <object-name>poticode</object-name>
    <object-name>potimaster</object-name>
    <object-name>potireply</object-name>
    <object-name>potitest</object-name>
    <object-name>switchaudiolight</object-name>
    <object-name>switchidentmic</object-name>
    <object-name>switchm1</object-name>
    <object-name>switchm2</object-name>
    <object-name>switchm3</object-name>
    <object-name>switchmc</object-name>
    <object-name>switchonoff</object-name>
    <object-name>switchradtestmon</object-name>
    <object-name>wheel1</object-name>
    <object-name>wheel2</object-name>
    <object-name>wheel3</object-name>
    <object-name>wheel4</object-name>
    <object-name>wheel5</object-name>
    <object-name>wheel6</object-name>
    <emission>
      <factor-prop>controls/lighting/instltg/ped</factor-prop>
      <red>  0.01</red>
      <green>0.01</green>
      <blue> 0.01</blue>
    </emission>
  </animation>

