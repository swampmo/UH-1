  <animation>
    <type>knob</type>
    <object-name>gainswitch</object-name>
    <property>controls/switches/arn83/gainswitch</property>
    <center>
      <x-m> 0.016</x-m>
      <y-m>-0.046</y-m>
      <z-m> 0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
    <factor>-27</factor>
    <action>
      <binding>
        <command>property-adjust</command>
        <property>controls/switches/arn83/gainswitch</property>
        <min>0</min>
        <max>3</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

