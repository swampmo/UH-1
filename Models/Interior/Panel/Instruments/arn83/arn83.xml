  <animation>
    <type>knob</type>
    <object-name>freqpreselectpoti</object-name>
    <property>instrumentation/adf/frequencies/freqbandselect</property>
    <center>
      <x-m>0.016</x-m>
      <y-m>0.047</y-m>
      <z-m>0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
    <factor>-49</factor>
    <increase>
      <binding>
        <command>nasal</command>
        <script>arn83.adjustadfband( 0 )</script>
      </binding>
    </increase>
    <decrease>
      <binding>
        <command>nasal</command>
        <script>arn83.adjustadfband( 1 )</script>
      </binding>
    </decrease>
  </animation>


  <animation>
    <type>knob</type>
    <object-name>tunepoti</object-name>
    <property>instrumentation/adf/frequencies/freqpos</property>
    <center>
      <x-m>0.016</x-m>
      <y-m>0.047</y-m>
      <z-m>0.000</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
    <factor>-0.5</factor>
    <increase>
      <binding>
        <command>nasal</command>
        <script>arn83.adjustadffreq( 0 )</script>
      </binding>
    </increase>
    <decrease>
      <binding>
        <command>nasal</command>
        <script>arn83.adjustadffreq( 1 )</script>
      </binding>
    </decrease>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>freqband</object-name>
    <property>instrumentation/adf/frequencies/freqpos</property>
    <offset-deg>20</offset-deg>
    <factor>0.0459</factor>
    <center>
      <x-m> 0.000</x-m>
      <y-m> 0.000</y-m>
      <z-m>-0.023</z-m>
    </center>
    <axis>
      <x>1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>freqband</object-name>
    <property>/instrumentation/adf/frequencies/freqbandselect</property>
    <factor>1.075</factor>
    <axis>
      <x>0</x>
      <y>1</y>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>pointer</object-name>
    <property>instrumentation/adf/in-range</property>
    <center>
      <x-m>-0.018</x-m>
      <y-m> 0.050</y-m>
      <z-m>-0.001</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
    <factor>-90</factor>
  </animation>

</PropertyList>
