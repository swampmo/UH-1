<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <chase-distance-m type="double" archive="y"> -20 </chase-distance-m>
  
  <view n="100">
    <name>Co pilot</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <dynamic-view type="bool">true</dynamic-view>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y"> -0.51 </x-offset-m> <!--right-->
      <y-offset-m archive="y"> -0.11 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -6.64 </z-offset-m> <!--back-->
      <pitch-offset-deg> -20 </pitch-offset-deg>
      <default-field-of-view-deg> 97.5 </default-field-of-view-deg>
    </config>
  </view>
  
  <view n="101">
    <name>Crew Chief</name>
    <internal archive="y">false</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y"> -0.61 </x-offset-m> <!--right-->
      <y-offset-m archive="y"> -0.11 </y-offset-m> <!--up-->
      <z-offset-m archive="y"> -5.7 </z-offset-m> <!--back-->
      <pitch-offset-deg> -20 </pitch-offset-deg>
      <default-field-of-view-deg> 97.5 </default-field-of-view-deg>
	   <heading-offset-deg>300</heading-offset-deg>
    </config>
  </view>
  
